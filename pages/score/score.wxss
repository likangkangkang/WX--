/* pages/score/score.wxss */
view {
  box-sizing: border-box;
}
.border {
  background-color: #eee;
  width: 100%;
  height: 1rpx;
}
.contain {
  padding: 40rpx 4%;
  width: 100%;
  height: 150rpx;
}
.button {
  color: white;
  background-color: #0099ff;
  margin-top: 40rpx;
}
.classList {
  padding: 20rpx 0;
  width: 100%;
  height: 140rpx;
  border-bottom: 1rpx solid #eee;
  position: relative;
}
.kcm {
  font-size: 32rpx;
}
.khfs {
  position: absolute;
  border: 1px solid #000;
  font-size: 24rpx;
  padding: 5rpx;
  width: 60rpx;
  top: 20rpx;
  right: 10rpx;
}
.xf, .fs {
  font-size: 28rpx;
  margin-top: 30rpx;
}
.xf {
  float: left;
}
.fs {
  float: right;
}
.jd {
  margin: 40rpx 0;
  float: right;
}
